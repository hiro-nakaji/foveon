= paginator.render do
  .pagination(style="display: inline; margin: 0px;")
    %ul
      %li
        = ActionController::Base.helpers.link_to('', Rails.application.routes.url_helpers.trees_messages_path, class: 'icon-home')
      %li
        = ActionController::Base.helpers.link_to('', Rails.application.routes.url_helpers.new_message_path, class: 'icon-file')
      = first_page_tag unless current_page.first?
      = prev_page_tag unless current_page.first?
      - each_page do |page|
        - if page.left_outer? || page.right_outer? || page.inside_window?
          = page_tag page
        - elsif !page.was_truncated?
          = gap_tag
      = next_page_tag unless current_page.last?
      = last_page_tag unless current_page.last?

